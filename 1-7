import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner stdIn = new Scanner(System.in);
        int n = stdIn.nextInt();
        int sum = 0;
        for (int i = 1; i < n; i++) {
            sum += i;
            System.out.print(i+" + ");
        }
        System.out.println(n +" = "+(sum+n));
    }
}
